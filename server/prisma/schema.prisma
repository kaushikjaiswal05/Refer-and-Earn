generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Referral {
  id            Int      @id @default(autoincrement())
  referrerName  String
  referrerEmail String
  refereeName   String
  refereeEmail  String
  createdAt     DateTime @default(now())
}
